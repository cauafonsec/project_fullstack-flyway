CREATE TABLE pessoa(	
	id BIGINT(20) PRIMARY KEY AUTO_INCREMENT,
	nome VARCHAR(155) NOT NULL,
	status VARCHAR(5) NOT NULL,
	logradouro VARCHAR(155),
	numero VARCHAR(155),
	complemento VARCHAR(155),
	bairro VARCHAR(155),
	cep VARCHAR(155),
	cidade VARCHAR(155),
	estado VARCHAR(155)
	) ENGINE=InnoDB DEFAULT CHARSET=utf8;
	
